{
  "manifest_version": 2,
  "name": "SafeBrowse",
  "description": "This extension blocks nsfw content!",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "webRequestBlocking",
    "storage",
    "declarativeNetRequest",
    "hostPermissions",
    "declarativeNetRequest",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "dbit.png",
    "default_popup": "window.html"
  }
}